#!/usr/bin/env python3
# main.py
import typer
from typer_htcondor import eventlog, job

app = typer.Typer()
app.add_typer(eventlog.eventlog_app, name="eventlog",
              help="Interacts with eventlogs")
app.add_typer(job.job_app, name="job", help="Run operations on HTCondor jobs")

if __name__ == "__main__":
    app()
